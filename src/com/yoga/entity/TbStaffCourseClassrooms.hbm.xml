<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.yoga.entity.TbStaffCourseClassrooms" table="tb_staff_course_classrooms" catalog="db_yoga">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <many-to-one name="tbStaff" class="com.yoga.entity.TbStaff" fetch="select">
            <column name="staff_id" length="32" />
        </many-to-one>
        <many-to-one name="tbCourse" class="com.yoga.entity.TbCourse" fetch="select">
            <column name="course_id" length="32" />
        </many-to-one>
        <many-to-one name="tbClassrooms" class="com.yoga.entity.TbClassrooms" fetch="select">
            <column name="classrooms_id" length="32" />
        </many-to-one>
        <property name="createTime" type="java.sql.Timestamp">
            <column name="create_time" length="19">
                <comment>使用时间</comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
